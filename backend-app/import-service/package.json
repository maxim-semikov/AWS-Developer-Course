{
    "name": "import-service",
    "version": "0.1.0",
    "bin": {
        "import-service": "bin/import-service.js"
    },
    "engines": {
        "node": ">=22.0.0 <23.0.0"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "deploy": "npm run build && cdk deploy",
        "destroy": "cdk destroy"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.test.ts"
        ],
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        }
    },
    "devDependencies": {
        "@aws-sdk/lib-dynamodb": "^3.758.0",
        "@aws-sdk/util-stream-node": "^3.370.0",
        "@types/aws-lambda": "^8.10.92",
        "@types/aws-sdk": "^0.0.42",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.1.7",
        "aws-cdk": "2.x",
        "aws-sdk-mock": "^6.2.1",
        "esbuild": "^0.25.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "~5.0.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.758.0",
        "@aws-sdk/s3-request-presigner": "^3.758.0",
        "@aws-sdk/types": "^3.734.0",
        "aws-cdk-lib": "2.x",
        "aws-sdk": "^2.1692.0",
        "constructs": "^10.0.0",
        "csv-parser": "^3.2.0",
        "dotenv": "^16.0.0",
        "source-map-support": "^0.5.21"
    }
}